<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% @item_orders.each do |item| %>
  <tbody>
      <tr>
        <td><%= item.title %></td>
        <td><%= item.description %></td>
        <td><%= item.price %></td>
        <td><%= image_tag(item.image_url) %></td>
      </tr>
  </tbody>
  <% end %>
</table>
